{
  "name": "orders-app",
  "version": "1.0.0",
  "description": "",
  "author": "YFLooi",
  "license": "ISC",
  "scripts": {
    "local": "NODE_ENV=local nodemon --trace-warnings --signal SIGTERM ./built/server.js",
    "dev": "NODE_ENV=development REDIS_ENV=local nodemon --trace-warnings --signal SIGTERM ./built/server.js",
    "tsc-watch": "tsc -w",
    "test": "jest --runInBand",
    "docker-build": "docker-compose -f docker/docker-compose.yml build"
  },
  "_moduleAliases": {
    "@src": "built/"
  },
  "jest": {
    "testEnvironment": "node",
    "moduleNameMapper": {
      "@src/(.*)$": "<rootDir>/src/$1"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testResultsProcessor": "jest-junit",
    "reporters": [
      "default",
      "jest-junit"
    ],
    "testTimeout": 10000,
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$"
  },
  "dependencies": {
    "@nestjs/common": "^8.0.6",
    "@nestjs/core": "^8.0.6",
    "@nestjs/cli": "^8.1.1",
    "@nestjs/schematics": "^8.0.2",
    "@nestjs/testing": "^8.0.6",
    "@types/express": "^4.17.13",
    "@types/node": "^16.7.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "@types/app-root-path": "^1.2.4",
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.10",
    "@types/dotenv": "^8.2.0",
    "@types/express-validator": "^3.0.0",
    "@types/http-errors": "^1.8.0",
    "@types/http-status-codes": "^1.2.0",
    "@types/jest": "^26.0.21",
    "@types/lodash": "^4.14.168",
    "@types/moment": "^2.13.0",
    "@types/mongoose": "5.10.4",
    "@types/mongoose-unique-validator": "1.0.4",
    "app-root-path": "^3.0.0",
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.10.0",
    "express-winston": "^4.1.0",
    "http-errors": "^1.8.0",
    "http-status-codes": "^2.1.4",
    "jest": "^27.0.3",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "mongodb-memory-server": "7.0.0",
    "mongoose": "5.10.3",
    "mongoose-unique-validator": "2.0.3",
    "nodemon": "^2.0.7",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.3.0",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.2.3"
  },
  "devDependencies": {
    "jest-junit": "^12.2.0"
  }
